<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
	<metadata>
		<id>HadoukInput</id>
		<version>1.0.4</version>
		<title>HadoukInput</title>
		<authors>dmanning23</authors>
		<owners>dmanning23</owners>
		<projectUrl>https://github.com/dmanning23/HadoukInput</projectUrl>
		<licenseUrl>https://github.com/dmanning23/HadoukInput/blob/master/LICENSE.txt</licenseUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>MonoGame input library for sanitizing and pattern matching of controller input</description>
		<releaseNotes />
		<copyright>Copyright 2016</copyright>
		<tags>pcl, windows phone, winphone, android, ios, windows universal, monogame</tags>
		<dependencies>

			<!--Xamarin.Android-->
			<group targetFramework="MonoAndroid10">
				<dependency id="MonoGame.Framework.Android" version="3.5"/>
				<dependency id="FilenameBuddy" version="1.0.1"/>
				<dependency id="MatrixExtensions" version="1.0"/>
				<dependency id="RandomExtensions.dmanning23" version="1.0"/>
				<dependency id="Vector2Extensions" version="1.0.1"/>
				<dependency id="XmlBuddy" version="1.0.1"/>
			</group>

			<!--Xamarin.iOS Unified-->
			<group targetFramework="Xamarin.iOS10">
				<dependency id="MonoGame.Framework.iOS" version="3.5"/>
				<dependency id="FilenameBuddy" version="1.0.1"/>
				<dependency id="MatrixExtensions" version="1.0"/>
				<dependency id="RandomExtensions.dmanning23" version="1.0"/>
				<dependency id="Vector2Extensions" version="1.0.1"/>
				<dependency id="XmlBuddy" version="1.0.1"/>
			</group>

			<!--Win Universal-->
			<group targetFramework="uap10.0">
				<dependency id="MonoGame.Framework.WindowsUniversal" version="3.5"/>
				<dependency id="Microsoft.NETCore.UniversalWindowsPlatform" version="5.1"/>
				<dependency id="FilenameBuddy" version="1.0.1"/>
				<dependency id="MatrixExtensions" version="1.0"/>
				<dependency id="RandomExtensions.dmanning23" version="1.0"/>
				<dependency id="Vector2Extensions" version="1.0.1"/>
				<dependency id="XmlBuddy" version="1.0.1"/>
			</group>

			<group targetFramework="net451">
				<dependency id="MonoGame.Framework.DesktopGL" version="3.5"/>
				<dependency id="FilenameBuddy" version="1.0.1"/>
				<dependency id="MatrixExtensions" version="1.0"/>
				<dependency id="RandomExtensions.dmanning23" version="1.0"/>
				<dependency id="Vector2Extensions" version="1.0.1"/>
				<dependency id="XmlBuddy" version="1.0.1"/>
			</group>

		</dependencies>
	</metadata>
	<files>

		<!--Xamarin.Android-->
		<file src="HadoukInput.Android\bin\Release\HadoukInput.Android.pdb" target="lib\MonoAndroid10\HadoukInput.Android.pdb" />
		<file src="HadoukInput.Android\bin\Release\HadoukInput.Android.dll" target="lib\MonoAndroid10\HadoukInput.Android.dll" />

		<!--Xamarin.iOS Unified-->
		<file src="HadoukInput.iOS\bin\Release\HadoukInput.iOS.pdb" target="lib\Xamarin.iOS10\HadoukInput.iOS.pdb" />
		<file src="HadoukInput.iOS\bin\Release\HadoukInput.iOS.dll" target="lib\Xamarin.iOS10\HadoukInput.iOS.dll" />

		<!--Windows Universal-->
		<file src="HadoukInput.WindowsUniversal\bin\Release\HadoukInput.WindowsUniversal.pdb" target="lib\uap10.0\HadoukInput.WindowsUniversal.pdb" />
		<file src="HadoukInput.WindowsUniversal\bin\Release\HadoukInput.WindowsUniversal.dll" target="lib\uap10.0\HadoukInput.WindowsUniversal.dll" />

		<!--DesktopGL-->
		<file src="HadoukInput.DesktopGL\bin\Release\HadoukInput.DesktopGL.pdb" target="lib\net451\HadoukInput.DesktopGL.pdb" />
		<file src="HadoukInput.DesktopGL\bin\Release\HadoukInput.DesktopGL.dll" target="lib\net451\HadoukInput.DesktopGL.dll" />

	</files>
</package>
